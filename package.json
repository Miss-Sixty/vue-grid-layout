{
  "name": "vue3-grid",
  "type": "module",
  "version": "0.0.2",
  "private": false,
  "description": "可拖动的vue3网格布局",
  "keywords": [
    "grid",
    "vuejs",
    "drag",
    "draggable"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/Miss-Sixty/vue3-grid.git"
  },
  "author": "xh",
  "homepage": "https://github.com/Miss-Sixty/vue3-grid",
  "main": "./dist/vue3-grid.umd.cjs",
  "module": "./dist/vue3-grid.js",
  "scripts": {
    "dev": "vite",
    "build": "run-p type-check build-only",
    "preview": "vite preview",
    "build-only": "vite build",
    "type-check": "vue-tsc --noEmit",
    "lint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix --ignore-path .gitignore",
    "format": "prettier --write src/",
    "pub": "pnpm publish"
  },
  "dependencies": {
    "@vueuse/core": "^10.1.2",
    "vue": "^3.2.47"
  },
  "devDependencies": {
    "@rushstack/eslint-patch": "^1.2.0",
    "@types/node": "^18.14.2",
    "@vitejs/plugin-vue": "^4.0.0",
    "@vue/eslint-config-prettier": "^7.1.0",
    "@vue/eslint-config-typescript": "^11.0.2",
    "@vue/tsconfig": "^0.1.3",
    "eslint": "^8.34.0",
    "eslint-plugin-vue": "^9.9.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.8.4",
    "sass": "^1.62.1",
    "typescript": "~4.8.4",
    "vite": "^4.1.4",
    "vite-plugin-dts": "^2.3.0",
    "vue-tsc": "^1.2.0"
  }
}
